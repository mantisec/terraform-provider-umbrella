# Terraform Variables Example File
# Copy this file to terraform.tfvars and fill in your actual values
# DO NOT commit terraform.tfvars to version control

# Umbrella API Credentials
# Get these from your Umbrella dashboard under Admin > API Keys
umbrella_api_key    = "your-umbrella-api-key-here"
umbrella_api_secret = "your-umbrella-api-secret-here"
umbrella_org_id     = "your-umbrella-org-id-here"

# Tunnel Configuration
# IP address of your network device that will connect to Umbrella SIG
tunnel_device_ip = "203.0.113.10"

# Pre-shared key for IPSec tunnel authentication
# Use a strong, randomly generated key
tunnel_pre_shared_key = "your-secure-pre-shared-key-here"

# Example values for different scenarios:

# For on-premises firewall
# tunnel_device_ip = "198.51.100.1"

# For cloud-based appliance
# tunnel_device_ip = "203.0.113.50"

# Multiple device IPs for redundant tunnels
# primary_device_ip   = "203.0.113.10"
# secondary_device_ip = "203.0.113.11"
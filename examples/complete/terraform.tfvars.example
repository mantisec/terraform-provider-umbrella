# Terraform Variables Example for Complete Umbrella Deployment
# Copy this file to terraform.tfvars and update with your actual values
# NEVER commit terraform.tfvars to version control

# Umbrella API Credentials
# Obtain these from your Umbrella dashboard: Admin > API Keys
umbrella_api_key    = "your-api-key-here"
umbrella_api_secret = "your-api-secret-here"
umbrella_org_id     = "your-org-id-here"

# User Account Passwords
# Use strong, unique passwords for each account type
admin_password    = "SecureAdminPassword123!"
readonly_password = "SecureReadOnlyPassword456!"

# Company Configuration
# Update with your actual company domain
company_domain = "yourcompany.com"

# Example values:
# umbrella_api_key    = "abcd1234-5678-90ef-ghij-klmnopqrstuv"
# umbrella_api_secret = "wxyz9876-5432-10ab-cdef-ghijklmnopqr"
# umbrella_org_id     = "1234567"
# admin_password      = "MySecureAdminPass2024!"
# readonly_password   = "MySecureReadOnlyPass2024!"
# company_domain      = "acmecorp.com"
{
	"name": "dev",
	"dockerComposeFile": "docker-compose.yml",
	"service": "dev",
	"workspaceFolder": "/workspaces",
	"mounts": [
		"type=bind,source=${localWorkspaceFolder},target=/workspaces,consistency=cached",
		"source=${localEnv:APPDATA}/Code/User/globalStorage,target=/home/vscode/.vscode-server/data/User/globalStorage,type=bind,consistency=cached",
		"source=${localEnv:APPDATA}/Code/User/settings.json,target=/home/vscode/.vscode-server/data/User/settings.json,type=bind,consistency=cached"
	],
	"customizations": {
		"vscode": {
			"settings": {
				"go.toolsManagement.checkForUpdates": "local",
				"go.useLanguageServer": true,
				"go.gopath": "/go",
				"go.goroot": "/usr/local/go",
				"go.lintTool": "golangci-lint",
				"go.formatTool": "goimports",
				"go.lintOnSave": "package",
				"go.vetOnSave": "package",
				"go.buildOnSave": "package",
				"go.testFlags": ["-v"],
				"go.testTimeout": "30s",
				"terminal.integrated.defaultProfile.linux": "zsh",
				"terminal.integrated.profiles.linux": {
					"zsh": {
						"path": "/bin/zsh",
						"args": ["-l"]
					}
				},
				"files.eol": "\n",
				"files.insertFinalNewline": true,
				"files.trimTrailingWhitespace": true,
				"editor.formatOnSave": true,
				"editor.codeActionsOnSave": {
					"source.organizeImports": true
				}
			},
			"extensions": [
				"golang.go",
				"hashicorp.terraform",
				"ms-vscode.makefile-tools",
				"ms-vscode.vscode-json",
				"ms-vscode.remote-containers",
				"rooveterinaryinc.roo-code"
			]
		}
	},
	"containerEnv": {
		"GO111MODULE": "on",
		"GOPROXY": "https://proxy.golang.org,direct",
		"GOSUMDB": "sum.golang.org",
		"CGO_ENABLED": "0",
		"TF_LOG": "INFO",
		"TF_CLI_CONFIG_FILE": "/workspaces/${localWorkspaceFolderBasename}/.terraformrc"
	}

}
